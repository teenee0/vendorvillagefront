# Production environment variables
# Эти переменные используются при сборке для продакшена (npm run build)

# API Base URL для продакшена
VITE_API_BASE_URL=https://api.axione.store

# Media Base URL для продакшена (если отличается от API URL)
VITE_MEDIA_BASE_URL=https://api.axione.store

# Название приложения
VITE_APP_NAME=Axione

# Режим отладки (false для продакшена)
VITE_DEBUG=false

# Уровень логирования (error для продакшена)
VITE_LOG_LEVEL=error
